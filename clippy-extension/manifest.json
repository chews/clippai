   },
   "browser_action": {
      "default_icon": {
         "16": "assets/img/clippy-icon-gray-48x48.png",
         "24": "assets/img/clippy-icon-gray-48x48.png",
         "32": "assets/img/clippy-icon-gray-48x48.png"
      },
      "default_title": "Clipp-ai Crypto Assistent"
   },
   "content_scripts": [ {
      "css": [ "assets/css/clippy.css" ],
      "js": [ "assets/js/jquery.min.js", "assets/js/clippy.js", "assets/js/agent.js", "lib/index.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   } ],
   "description": "DESC HERE",
   "externally_connectable": {
